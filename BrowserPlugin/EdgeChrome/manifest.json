{
  "manifest_version": 2,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Detects phishing websites using an ML model.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}